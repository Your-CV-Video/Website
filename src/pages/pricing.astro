---

<Layout>
	
	<!-- Pricing -->
	<PricingSec 	relativeClass="relative"  backgroupClass="h-1/3 lg:h-[48rem] [clip-path:polygon(0_0,_5760px_0,_5760px_calc(100%_-_352px),_0_100%)]"/>

    <!-- Startup promo -->
   

     <!-- FAQs -->
   
    <FaqSec class="bg-slate-50"/>

        <!-- CTA -->

    <CtaSec relativeClass="bg-slate-900" headerColorClass="text-slate-100" paraColorClass="text-slate-400" />


</Layout>
